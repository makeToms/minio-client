{
  "groups": [
    {
      "name": "spring.io.minio",
      "type": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties"
    }
  ],
  "properties": [
    {
      "name": "spring.io.minio.bucket-name",
      "type": "java.lang.String",
      "description": "使用的桶名",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": "temp"
    },
    {
      "name": "spring.io.minio.buckets",
      "type": "java.util.List<java.lang.String>",
      "description": "桶名",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties"
    },
    {
      "name": "spring.io.minio.host",
      "type": "java.lang.String",
      "description": "主机IP",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": "127.0.0.1"
    },
    {
      "name": "spring.io.minio.max-idle",
      "type": "java.lang.Integer",
      "description": "对象池中最大的空闲对象个数。默认值是8",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": 10
    },
    {
      "name": "spring.io.minio.max-total",
      "type": "java.lang.Integer",
      "description": "对象池中管理的最多对象个数。默认值是8",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": -1
    },
    {
      "name": "spring.io.minio.min-evictable-idle-time-millis",
      "type": "java.lang.Integer",
      "description": "连接池中的连接最小空闲时间(池中最小的空闲时间，空闲半小时后直接销毁)",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": 30000
    },
    {
      "name": "spring.io.minio.min-idle",
      "type": "java.lang.Integer",
      "description": "对象池中最小的空闲对象个数。默认值是0",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": 10
    },
    {
      "name": "spring.io.minio.num-tests-per-eviction-run",
      "type": "java.lang.Integer",
      "description": "每次检查空闲对象的个数，如果小于0，表示检查所有空闲对象。默认是3个",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": 3
    },
    {
      "name": "spring.io.minio.password",
      "type": "java.lang.String",
      "description": "minioadmin",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": "minioadmin"
    },
    {
      "name": "spring.io.minio.path-prefix",
      "type": "java.util.Map<java.lang.String,java.lang.String>",
      "description": "文件隔离前缀，在同一个桶中生效",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties"
    },
    {
      "name": "spring.io.minio.port",
      "type": "java.lang.Integer",
      "description": "端口",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": 9000
    },
    {
      "name": "spring.io.minio.time-between-eviction-runs-millis",
      "type": "java.lang.Integer",
      "description": "检查空闲对象和维护连接池的时间间隔。这个参数默认是1分钟。",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": 6000
    },
    {
      "name": "spring.io.minio.username",
      "type": "java.lang.String",
      "description": "minioadmin",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": "minioadmin"
    },
    {
      "name": "spring.io.minio.while-idle",
      "type": "java.lang.Boolean",
      "description": "检查对象是否有效，默认为false（不检查）",
      "sourceType": "org.spring.boot.autoconfigure.properties.MinioProperties",
      "defaultValue": false
    }
  ],
  "hints": []
}